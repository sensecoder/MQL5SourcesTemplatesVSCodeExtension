      "mqh": {
         "Templates": {
            "Class": {
               "PrototypeFileName": {
                  "UserPresentation": {
                     "hide": true
                  },
                  "PrototypeFileName":"mql5_class.tmp"
               },
               "Description": {
                  "UserPresentation": {
                     "checkBoxField": {
                        "checker": "IncludeDescription",
                        "caption": "Include description:",
                        "content": [
                           {
                              "container": "textarea",
                              "value": "Description"
                           }
                        ]
                     }
                  },
                  "IncludeDescription": false,
                  "Description": "None $FileName Description"
               },               
               "ClassName":"$FileName",
               "ExtendClass": {
                  "UserPresentation": {
                     "checkBoxField": {
                        "checker": "Extend",
                        "caption": "Parent class extend:",
                        "content": [
                           {
                              "container":"valueEdit",
                              "value":"RootClassName"
                           },
                           {
                              "container": "select",
                              "caption": "Modifier:",
                              "value": "ExtendModifier",
                              "selectedValue": "Modifier"
                           }
                        ]
                     }
                  },
                  "Extend": false,
                  "ExtendModifier": [
                     "public", "protected", "private"
                  ],
                  "Modifier": "public",
                  "RootClassName":"RootClassName"
               }
            }
         }
      }
   }
