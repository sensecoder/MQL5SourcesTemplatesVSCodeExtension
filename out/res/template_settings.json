{
   "General": {
      "Copyright": "Copyright 2021, Anton Staroverkin",
      "Link": "https://www.mql5.com/ru/users/sensecoder"
   },
   "FileExtension": {
      "mq5": {
         "Templates": {
            "Expert": {
               "PrototypeFileName": {
                  "UserPresentation": {
                     "hide": true
                  },
                  "Value":"mql5_expert.tmp"
               },
               "IncludeDescription": false,
               "OnTimer": false,
               "OnTrade": false,
               "OnTradeTransaction": false,
               "OnTester": false,
               "OnTesterInit": false,
               "OnTesterPass": false,
               "OnTesterDeinit": false,
               "OnChartEvent": true,
               "OnBookEvent": false
            }
         }
      },
      "mqh": {
         "Templates": {
            "Class": {
               "PrototypeFileName": {
                  "UserPresentation": {
                     "hide": true
                  },
                  "PrototypeFileName":"mql5_class.tmp"
               },
               "Description": {
                  "UserPresentation": {
                     "checkBoxActivateField": {
                        "checker": "IncludeDescription",
                        "caption": "Include description:",
                        "content": {
                           "container": "textEdit",
                           "value": "Description"
                        }
                     }
                  },
                  "IncludeDescription": false,
                  "Description": ""
               },               
               "ClassName":"$FileName",
               "ExtendClass": {
                  "UserPresentation": {
                     "checkBoxActivateField": {
                        "checker": "Extend",
                        "caption": "Parent class extend:",
                        "content": [
                           {
                              "container":"valueEdit",
                              "value":"RootClassName"
                           },
                           {
                              "container": "dropBox",
                              "caption": "Modifier:",
                              "value": "ExtendModifier"
                           }
                        ]
                     }
                  },
                  "Extend": false,
                  "ExtendModifier": [
                     "public", "protected", "private"
                  ],
                  "RootClassName":"RootClassName"
               }
            }
         }
      }
   }
}